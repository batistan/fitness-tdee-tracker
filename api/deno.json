{
  "name": "@calometri/api",
  "version": "0.0.1",
  "exports": "./src/main.ts",
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --watch src/main.ts",
    "start": "deno run --allow-net --allow-env --allow-read src/main.ts",
    "test": "deno test --allow-env --allow-read",
    "test:watch": "deno test --allow-env --allow-read --watch",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:studio": "npx drizzle-kit studio"
  },
  "imports": {
    "@src/": "./src/",
    "@tests/": "./tests/",
    "@shared/": "../shared/",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/testing": "jsr:@std/testing@^1.0.0",
    "hono": "jsr:@hono/hono@^4.7.0",
    "zod": "npm:zod@^3.24.0",
    "drizzle-orm": "npm:drizzle-orm@^0.38.0",
    "@neondatabase/serverless": "npm:@neondatabase/serverless@^0.10.0",
    "drizzle-kit": "npm:drizzle-kit@^0.30.0"
  }
}
